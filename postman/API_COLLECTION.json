			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var url = pm.environment.get(\"APIURL\") + \"/\" + pm.environment.get(\"PREFIX\") + \"/token\"",
							"",
							"var req = {",
							"    url,",
							"    method: 'POST',",
							"    header: {",
							"        'Content-Type': 'application/json',",
							"        'Authorization': 'Basic am9obkBleGFtcGxlLmNvbTpleGFtcGxlUGFzc3dvcmQxMjM0'}",
							"}",
							"pm.sendRequest(req, (error, response) => {",
							"        var jsonResponse = response.json();",
							"        var token = jsonResponse.token;",
							"        pm.environment.set('JWT', token);",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
